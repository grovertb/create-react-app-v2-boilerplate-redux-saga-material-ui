{
  "name": "create-react-app-v2-boilerplate-redux-saga-material-ui",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://grovertb.github.io/create-react-app-v2-boilerplate-redux-saga-material-ui",
  "dependencies": {
    "gh-pages": "^2.0.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts": "2.0.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "prod": "npm run build && npm run deploy",
    "predeploy": "npm run build",
    "deploy": "gh-pages -b gh-pages -d build",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier-eslint --write \"{src,server}/**/*.js\" && eslint . --ext .js --fix",
    "lint-staged": "lint-staged"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "{src,server}/**/*.js": [
      "prettier-eslint --write \"{src,server}/**/*.js\"",
      "eslint . --ext .js --fix",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "babel-plugin-transform-imports": "^1.5.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.18",
    "eslint": "^5.6.1",
    "eslint-plugin-react": "^7.11.1",
    "lint-staged": "^7.3.0",
    "pre-commit": "^1.2.2",
    "prettier-eslint-cli": "^4.7.1",
    "react-app-rewired": "^1.6.2"
  }
}
