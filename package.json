{
  "name": "create-react-app-v2-boilerplate-redux-saga-material-ui",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://grovertb.github.io/create-react-app-v2-boilerplate-redux-saga-material-ui",
  "dependencies": {
    "@material-ui/core": "3.2.0",
    "@material-ui/icons": "^3.0.1",
    "cross-env": "^5.2.0",
    "extensible-duck": "^1.4.0",
    "gh-pages": "2.0.1",
    "history": "^4.7.2",
    "ignore-styles": "^5.0.1",
    "immer": "^1.7.2",
    "morgan": "^1.9.1",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "2.0.4",
    "redux": "^4.0.0",
    "redux-devtools": "^3.4.1",
    "redux-devtools-dock-monitor": "^1.1.3",
    "redux-devtools-log-monitor": "^1.4.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "0.16.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "serve": "cross-env NODE_ENV=production node ./server/index.js",
    "prod": "npm run build && npm run deploy",
    "predeploy": "npm run build",
    "deploy": "gh-pages -b gh-pages -d build",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier-eslint --write \"{src,server}/**/*.js\" && eslint . --ext .js --fix",
    "lint-staged": "lint-staged"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "{src,server}/**/*.js": [
      "prettier-eslint --write \"{src,server}/**/*.js\"",
      "eslint . --ext .js --fix",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "babel-plugin-dynamic-import-node-babel-7": "^2.0.7",
    "babel-plugin-transform-imports": "^1.5.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.18",
    "eslint-plugin-react": "^7.11.1",
    "lint-staged": "^7.3.0",
    "pre-commit": "^1.2.2",
    "prettier-eslint-cli": "^4.7.1"
  }
}
